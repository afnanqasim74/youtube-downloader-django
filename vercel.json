{
    "version": 2,
    "builds": [
      {
        "src": "djangoProject",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/djangoProject/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/djangoProject/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/djangoProject/wsgi.py"
      }
    ]
  }
  